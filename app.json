{
  "expo": {
    "name": "DateLink",
    "slug": "datelink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/DATELINK_LOGO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DATELINK_LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.datelink"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/DATELINK_LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.datelink",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow DateLink to use your location to show you people nearby.",
          "locationWhenInUsePermission": "Allow DateLink to use your location to show you people nearby."
        }
      ],
      "react-native-google-mobile-ads"
    ],
    "extra": {
      "eas": {
        "projectId": "599735c1-9efe-4e6b-8e42-8cb40c960050"
      }
    }
  }
}
